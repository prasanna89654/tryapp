generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model intern{
  id          String      @id @default(uuid())
  email       String      @unique
  name        String      @unique
  password    String
  phone       String
  address     String
  worktype    WorkType 
  framework   Frameworks 
  image       String?
  createdAt   DateTime    @default(now())
}

enum WorkType{
  Onsite
  Remote
  Hybrid
}

enum Frameworks{
    Angular
    Flutter
    React
    Nodejs
}




